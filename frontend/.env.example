# BARQ Fleet Management - Frontend Environment Configuration Template
# Copy this file to .env.development or .env.production

# API Configuration
VITE_API_URL=http://localhost:8000/api/v1

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Feature Flags
VITE_ENABLE_SW=false
VITE_ENABLE_PERF_MONITORING=true
VITE_ENABLE_ANALYTICS=false

# App Configuration
VITE_APP_NAME=BARQ Fleet Management
VITE_APP_VERSION=1.0.0

# Sentry Error Tracking
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/your-project-id
VITE_SENTRY_TRACES_SAMPLE_RATE=1.0

# Auth token for source map upload (production builds only - get from Sentry settings)
# SENTRY_AUTH_TOKEN=your-sentry-auth-token
